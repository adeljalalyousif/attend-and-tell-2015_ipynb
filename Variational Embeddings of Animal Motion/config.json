{
    "preprocessing": {
		"savgol": {
			"window_length": 5,
			"polyorder": 2
		}
	},
    "generator": {
        "window_size": 30,
        "shift": 1,
        "batch_size": 256,
        "dilation": 1,
        "shuffle": true
    },
    "model": {
        "latent_size": 30,
        "n_layers_encoder": [64, 64],
        "n_layers_decoder": [64],
        "encoder_bidirectional": true,
        "decoder_bidirectional": true,
        "soft_plus": false,
        "encoder_dropout": 0.0,
        "decoder_dropout": 0.0,
        "n_clusters": 15
    },
    "training": {
        "epochs": 500,
        "optimizer": "adam"
    },
    "community_detection": {
        "tree_cut": 2,
        "threshold": 0.0
    }
}